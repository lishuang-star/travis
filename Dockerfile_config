FROM debian:jessie
USER root

ADD File/config.sh /config.sh
ADD File/wait-for-it.sh /wait-for-it.sh
ADD File/requirements_config.txt /requirements_config.txt
ADD File/datafill.py /datafill.py
RUN mkdir /video
COPY File/video/.  /video

RUN chmod +x /config.sh 
CMD ["bash", "-c", "echo 1 && bash /config.sh"]
